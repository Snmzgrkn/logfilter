pipeline {
    agent any

    stages {
        stage('Setup') {
            steps {
                sh 'python3 -m pip install -r requirements.txt || true'
            }
        }

        stage('Run Script') {
            steps {
                sh 'python3 main.py'
            }
        }

        stage('Archive Output') {
            steps {
                archiveArtifacts artifacts: 'filtered_logs.csv', allowEmptyArchive: true
            }
        }
    }
}